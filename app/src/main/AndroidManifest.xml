        <activity android:name=".Three" android:screenOrientation="portrait" android:theme="@style/AppTheme"/>
        <activity android:name=".Four" android:screenOrientation="portrait" android:theme="@style/AppTheme"/>
        <activity android:name=".Five" android:screenOrientation="portrait" android:theme="@style/AppTheme"/>
        <activity android:name=".Six" android:screenOrientation="portrait" android:theme="@style/AppTheme"/>
        <activity android:name=".Seven" android:screenOrientation="portrait" android:theme="@style/AppTheme"/>
        <activity android:name=".Eight" android:screenOrientation="portrait" android:theme="@style/AppTheme"/>
        <activity android:name=".Nine" android:screenOrientation="portrait" android:theme="@style/AppTheme"/>
    </application>

</manifest>